{
    "name": "rainwaves/paygate-payment",
    "description": "A package for integrating with the PayGate payment gateway",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "rainwaves\\PaygatePayment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rainwaves\\PaygatePayment\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Joel Mnisi",
            "email": "joel.mnisi@protonmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "respect/validation": "^2.2",
        "guzzlehttp/guzzle": "^7.7",
        "nesbot/carbon": "^2.68"
    },
    "require-dev": {
        "orchestra/testbench": "^6.28",
        "friendsofphp/php-cs-fixer": "^3.14"
    },
    "extra": {
        "laravel": {
            "providers": [
                "rainwaves\\PaygatePayment\\PayGateServiceProvider"
            ],
            "config": [
                "paygate.php"
            ]
        }
    }
}
